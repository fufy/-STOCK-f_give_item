				{
					cs_set_user_bpammo(index, iWeaponTypeID, iBPAmmo);
				}
			}

			case CSW_KNIFE:
			{
				if( !user_has_weapon( index, CSW_KNIFE ) ) 
				{
					give_item( index, "weapon_knife" );
				}
			}

		}

	}
	
}
